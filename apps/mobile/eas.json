{
    "cli": {
        "version": ">= 15.0.15",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_BASE_URL": "http://localhost:3000"
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_BASE_URL": "https://fjios.vercel.app/"
            }
        }
    },
    "submit": {
        "production": {}
    }
}