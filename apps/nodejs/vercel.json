{
  "version": 2,
  "buildCommand": "npm run build || true",
  "outputDirectory": ".",
  "framework": null,
  "installCommand": "npm install",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD .",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

